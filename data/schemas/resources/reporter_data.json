{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "reporter_type": {
      "type": "string",
      "enum": [
        "HBI",
        "ACM",
        "OCM",
        "ACS",
        "NOTIFICATIONS"
      ]
    },
    "console_href": { "type": "string" },
    "api_href": { "type": "string" },
    "local_resource_id": { "type": "string" },
    "reporter_version": { "type": "string" }
  },
  "required": [
    "reporterType",
    "consoleHref",
    "apiHref",
    "localResourceId",
    "reporterVersion"
  ]
}

