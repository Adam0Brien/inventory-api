{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "hostname": {
      "type": "string"
    },
    "status": {
      "type": "string",
      "enum": [
        "ACTIVE",
        "INACTIVE",
        "DECOMMISSIONED",
        "MAINTENANCE"
      ]
    }
  },
  "required": [
    "hostname",
    "status"
  ]
}
